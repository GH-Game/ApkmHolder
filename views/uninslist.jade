  a#unins-submit.btn.btn-info.apkm-btn 确认
  a#unins-reset.btn.btn-warning.apkm-btn 清空
  #tips
    small Copyright@2015 GH-Game.
  img#logo(src='./img/sleep.jpg')

  script(src='./js/uninstall.js')
